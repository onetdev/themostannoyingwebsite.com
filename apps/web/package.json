{
  "private": true,
  "name": "@maw/web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:deployment-meta && pnpm run publish:assets && next build",
    "build:metadata": "pnpm run build:deployment-meta && pnpm run build:web-manifest",
    "build:deployment-meta": "tsx ./scripts/build-deployment-meta.ts",
    "build:web-manifest": "tsx ./scripts/build-web-manifest.ts",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "dev": "pnpm run build:deployment-meta && next dev --experimental-https",
    "lint": "biome check .",
    "format": "biome format --write .",
    "lint:fix": "biome check --write .",
    "publish:assets": "tsx ./scripts/publish-assets.ts",
    "start": "pnpm run build:deployment-meta && next start",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test"
  },
