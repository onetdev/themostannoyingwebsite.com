    "build:manifest": "tsx ./scripts/build-manifest.ts",
    "build:schema": "tsx ./scripts/build-json-schema.ts",
    "build:storybook": "storybook build",
    "build:artifacts": "pnpm run build:article-index && pnpm run build:schema",
    "build": "pnpm run build:artifacts && next build",
    "dev": "pnpm run build:artifacts && next dev --experimental-https",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "prepare": "husky",
    "postinstall": "pnpm run build:schema",
    "start": "pnpm run build:artifacts && next start",
    "storybook:dev": "storybook dev -p 6006",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "test:once": "jest",
    "test": "jest --watch",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
