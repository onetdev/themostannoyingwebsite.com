  "scripts": {
    "build:article-index": "tsx ./scripts/build-article-index.ts",
    "build:manifest": "tsx ./scripts/build-manifest.ts",
    "build:schema": "tsx ./scripts/build-json-schema.ts",
    "build:storybook": "storybook build",
    "build:deployment-meta": "tsx ./scripts/build-deployment-meta.ts",
    "build:artifacts": "pnpm run build:article-index && pnpm run build:schema && pnpm run build:deployment-meta",
    "build": "pnpm run build:artifacts && next build",
    "dev": "pnpm run build:artifacts && next dev --experimental-https",
    "lint:fix": "next lint --fix",
    "lint": "next lint",
    "prepare": "husky",
    "start": "pnpm run build:artifacts && next start",
    "storybook:dev": "storybook dev -p 6006",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "test:once": "jest",
    "test:e2e": "playwright test",
    "test": "jest",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
